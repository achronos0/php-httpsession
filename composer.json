{
	"name": "morvren-achronos/useful",
    "type": "library",
	"version": "0.1.1",
	"time" : "2010-05-21",
	"description": "Useful is a PHP non-framework. It's a collection use utility classes. Take what you want, leave the rest.",
	"homepage": "https://github.com/morvren-achronos/php-useful",
	"license" : "Apache-2.0",
	"authors": [
		{
			"name": "Ky Patterson",
			"email": "ky@morvren-achronos.com"
		}
	],
	"require": {
		"php": "^5.0 || ^7.0",
		"psr/log": "^1.1"
	},
	"suggest": {
		"ext-curl": "Required for Useful\\HttpClient"
	},
	"autoload": {
		"psr-4": {
			"Useful\\": "src/Useful/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^8.1"
	}
}
